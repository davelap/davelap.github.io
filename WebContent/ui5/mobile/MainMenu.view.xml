<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<mvc:View xmlns:core="sap.ui.core" 
			xmlns:mvc="sap.ui.core.mvc" 
			xmlns="sap.m"
			controllerName="ui5.mobile.MainMenu" 
			xmlns:html="http://www.w3.org/1999/xhtml">
		

            <TileContainer  xmlns="sap.m" id="container" tileDelete="handleTileDelete" width="100%" height="100%">
            	<tiles>
            		<StandardTile xmlns="sap.m" icon="sap-icon://list" 			type="None" title="Car Listing" 			info="List of cars" press="OpenCarList" />
            		<StandardTile xmlns="sap.m" icon="sap-icon://list" 			type="None" title="Car Listing" 			info="Desktop - List of cars" press="OpenDesktopCarList" />
                	<StandardTile xmlns="sap.m" icon="sap-icon://insurance-car" type="None" title="Car Listing Carousel" 	info="Car Carousel" press="OpenCarListCarousel" />
                	<StandardTile xmlns="sap.m" icon="sap-icon://retail-store" 	type="None" title="Find Dealer" 			info="Find Dealer" 	press="OpenFindDealer" />
                	<StandardTile xmlns="sap.m" icon="sap-icon://car-rental" 	type="None" title="Get Car Detail" 			info="Car Detail" 	press="OpenCarDetail" />
            	</tiles>
            </TileContainer>

</mvc:View>
